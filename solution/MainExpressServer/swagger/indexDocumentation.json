    "/getClubValue": {
      "get": {
        "summary": "Retrieve the market value of a club",
        "parameters": [
          {
            "name": "clubId",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "example": 11
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The market value of the club",
            "content": {
              "application/json": {
                "schema": {
                  "type": "integer",
                  "example": 1127100000
                }
              }
            }
          },
          "404": {
            "description": "No data found"
          },
          "500": {
            "description": "An error occurred while fetching data"
          }
        }
      }
